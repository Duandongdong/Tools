<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bestgood.commons">

    <uses-sdk />

    <!--  Normal permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!--Dangerous permissions -->
    <!--Permission Group PHONE-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />


    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name="com.bestgood.commons.ui.multiphotopicker.view.ImageZoomActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.NoTitleBar"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.bestgood.commons.ui.multiphotopicker.view.ImageChooseActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.NoTitleBar"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />

        <service android:name="com.amap.api.location.APSService" />
        <service android:name="com.bestgood.commons.network.http.HttpClientService" />
    </application>

</manifest>
